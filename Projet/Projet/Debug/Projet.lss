                                 ; T 	T=1 button press (transition up-down)
                                 ; Z	Z=1 button down change
                                 
0000d9 94e8                      	clt						; preclear T
0000da b111                      	in	_w,ENCOD-2			; read encoder port (_w=new)
                                 	
0000db 7710                      	andi	_w,0b01110000	; mask encoder lines (A,B,I)
0000dc 9020 0100                 	lds	_u,enc_old			; load prevous value (_u=old)
0000de 1512                      	cp	_w,_u				; compare new<>old ?
0000df f411                      	brne	PC+3
0000e0 9498                      	clz
